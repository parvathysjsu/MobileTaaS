  var connectProjectTable;
$(document).ready(function() {
  //var connectProjectTable=$('#project-table).DataTable();

  
  connectProjectTable = $('#project-table').DataTable( {
  "ajax": {
    "url": "getAllProjects",
    "type": "GET"
  },
  
  "columns": [
            { "data": "name" },
            { "data": "link1" },
            { "data": "documentation" },
            { "data": "is_joined",
            "render": function (data, type, row) {
              console.log(row.is_joined);
              if(row.is_joined==false){
                return "<button type='button' class='button-2'>Join</button> ";
              }
              else {
                return "<button type='button' class='button-2' style='background-color:red'>joined</button> ";
              }
              }
            },
            {
                        data: null,
                        className: "center",
                        defaultContent: '<a href= "/project_bugs"><button class="btn btn-danger" id=@TableRowId ><i class="fa fa-arrow-right"></i></button></a>'
                        //defaultContent: '<button onclick="myFunction()" class="btn btn-danger" id=@TableRowId ><i class="fa fa-arrow-right"></i></button>'
                    }
            //{ "data": "testcases" },
            //{ "data": "testcases" }
        ],

        
    });

  
  $('#project-table').on('click', 'tbody tr', function (e) {
      
      //var now = this.rowIndex;
      var now = this;
      var d =   connectProjectTable.row(this).data();
      alert(d.is_joined);
      console.log(d);
      if(d.is_joined==false){ 
        $.ajax({url: "join_project",
                type: "POST",
                data:d,
                success: function(result){
                  if(result){
			              alert("Successfully connected.");
                  
                  } else{
		                alert("some error occured");
		                window.location.href='home';
                }
               }
          })
      }else{
        $.ajax({
          url: '/disconnect-switch',
          method: 'DELETE',
          data:d,
  
          success: function(result) {
          if(result == "success"){
            alert("Successfully Disconnected:" + d.device_name); 
            
          }
          else {
            alert("Some error occured");
          }
    },
    error: function(request,msg,error) {
  
    }
  })
}

   

    })


